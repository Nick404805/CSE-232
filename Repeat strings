#include <iostream>
#include <string>
using std::cout;
using std::cin;
using std::stoi;

void repeat(std::string &s1) // input argument
{
	for (auto i = 0; i < 1; i++) // iterates until i is less than 1, i increment
	{
		s1 += s1; // s1 is adding to itself ( s1 = s1 + s1)
	}
}

