#include <iostream>
#include <string>
#include <iomanip> // for std::setw()
using std::cout;
using std::cin;
using std::endl;
using std::setw;

int main()
{
	int n;
	cin >> n;
	for (int i = 1; i < (n + 1); i++)
	{
		for (int j = 1; j < (n + 1); j++) 
		{
			cout << std::setw(4) << i * j;
		}
		cout << endl;
	}
}

/*
*	//https://www.tutorialspoint.com/cplusplus/cpp_nested_loops.htm
* //https://stackoverflow.com/questions/36165497/nested-for-loops-with-numbers-going-down-columns
* //https://www.thepolyglotdeveloper.com/2019/04/getting-familiar-loops-cpp/
* //https://en.cppreference.com/w/cpp/io/manip/setw
*/
